{
  "name": "css-grid-supports-wrapper",
  "version": "1.0.0",
  "description": "Processes CSS to wrap CSS grid usage in `@supports`. This is typically used to have browsers like IE11 fall back to a mobile design instead of trying to render with it's partial CSS Grid implementation.",
  "main": "./dist/index.js",
  "scripts": {
    "test": "eslint src/**/*.js && babel-node src/test.js",
    "build": "babel src --out-dir dist",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justafish/css-grid-supports-wrapper.git"
  },
  "keywords": [
    "cssgrid",
    "css-in-js",
    "stylis",
    "emotion"
  ],
  "author": "Sally Young <sally@justafish.co.uk> (http://www.justafish.co.uk)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/justafish/css-grid-supports-wrapper/issues"
  },
  "homepage": "https://github.com/justafish/css-grid-supports-wrapper#readme",
  "dependencies": {},
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "eslint": "^4.9.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "stylis": "^3.4.8"
  }
}
